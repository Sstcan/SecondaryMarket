<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:orientation="vertical"
    tools:context="com.example.shishengtao.secondarymarket.fragment.BlankFragment_my">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/message"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/colorLable"
            android:gravity="center"
            android:text="我"
            android:textSize="25sp" />
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/add_2"
            android:layout_alignParentRight="true"
           android:layout_centerVertical="true"
            android:layout_marginRight="2dp"

            />

    </RelativeLayout>

    //我的摊位

    <RelativeLayout
        android:id="@+id/MyRela_booth"
        android:layout_width="match_parent"
        android:background="@color/colorWhite"
        android:layout_height="40dp"
        android:layout_marginTop="170dp">
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/booth"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的摊位"
            android:textSize="20sp"
            android:layout_marginLeft="70dp"
            android:layout_centerVertical="true"/>
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/label_back"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>

    //求购
    <RelativeLayout
        android:id="@+id/MyRela_buy"
        android:layout_width="match_parent"
        android:background="@color/colorWhite"
        android:layout_height="40dp"
        android:layout_marginTop="220dp">
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/collect"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="收藏"
            android:textSize="20sp"
            android:layout_marginLeft="70dp"
            android:layout_centerVertical="true"/>
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/label_back"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>

    //收藏
    <RelativeLayout
        android:id="@+id/MyRela_colloct"
        android:layout_width="match_parent"
        android:background="@color/colorWhite"
        android:layout_height="40dp"
        android:layout_marginTop="270dp">
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/buy"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="求购"
            android:textSize="20sp"
            android:layout_marginLeft="70dp"
            android:layout_centerVertical="true"/>
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/label_back"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_alignParentBottom="true" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/MyRela_about"
        android:layout_width="match_parent"
        android:background="@color/colorWhite"
        android:layout_height="40dp"
        android:layout_marginTop="320dp">
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/about"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于我们"
            android:textSize="20sp"
            android:layout_marginLeft="70dp"
            android:layout_centerVertical="true"/>
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/label_back"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/MyRela_set"
        android:layout_width="match_parent"
        android:background="@color/colorWhite"
        android:layout_height="40dp"
        android:layout_marginTop="370dp">
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/set"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:textSize="20sp"
            android:layout_marginLeft="70dp"
            android:layout_centerVertical="true"/>
        <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/label_back"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/MyRela_login"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginTop="70dp"
        android:background="@color/colorWhite">

        <ImageView
            android:id="@+id/my_login"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"

            android:src="@drawable/add_1" />
    </RelativeLayout>

</FrameLayout>
